<section class="create-form-container">

  <form method="post">
    <h3>Create Book Review</h3>
    <label>Title</label>
    <input type="text" name="title" class="box" id="" value="{{enteredData.title}}" />
    <label>Author</label>
    <input type="text" name="author" class="box" id="" value="{{enteredData.author}}"  />
    <label>Genre</label>
    <input type="text" name="genre" class="box" id="" value="{{enteredData.genre}}" />
    <label>Stars</label>
    <input type="number" name="stars" class="box" id="" value="{{enteredData.stars}}" />
    <label>Image</label>
    <input type="text" name="image" class="box" id="" value="{{enteredData.image}}" />
    <label>Review</label>
    <textarea class="box" name="bookReview">value="{{enteredData.bookReview}}"</textarea>
    <input type="submit" value="Create" class="btn" />
  </form>
</section>